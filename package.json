{
  "name": "unsplash",
  "private": true,
  "version": "0.0.0",
  "module": "commonjs",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "nodemon --watch server.ts --exec 'ts-node --project tsconfig.node.json server.ts'",
    "prettier:check": "prettier --config prettierrc.json --check \"src/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --config prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/css": "^11.10.6",
    "axios": "^1.3.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "masonic": "^3.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-express": "^0.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}
